<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.saurmn.bookarchive" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name">
        <activity
            android:name=".BookList"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddEditAuthor" android:label="@string/app_name"></activity>
        <activity android:name=".AddEditBook" android:label="@string/app_name"></activity>
        <activity android:name=".ViewAuthor" android:label="@string/app_name"></activity>
        <activity android:name=".ViewBook" android:label="@string/app_name"></activity>
        <activity android:name=".AuthorList" android:label="@string/app_name"></activity>
        <activity android:name=".BookArchive" android:label="@string/app_name"></activity>
    </application>

</manifest>
